        "a, b, c = 10, 20, 30\n",
        "print(f'{a} == {b}: {a==b}')\n",
        "print(f'{a}!={b}: {a!=b}')\n",
        "print(f'{a} > {b}: {a>b}')\n",
        "print(f'{b} < {c}: {b<c}')\n",
        "print(f'{a}>={b}: {a>=b}')\n",
        "print(f'{b}<={c}: {b<=c}')"
      ]
